{
  "name": "resume-cli",
  "version": "0.0.0-development",
  "description": "The JSON Resume command line interface",
  "main": "index.js",
  "engines": {
    "node": ">=10.18.1"
  },
  "files": [
    "index.js",
    "lib/*"
  ],
  "scripts": {
    "dev": "babel-node lib/main.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --ignore-path .gitignore lib",
    "prepare": "babel lib -d build --copy-files",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsonresume/resume-cli.git"
  },
  "license": "MIT",
  "bin": {
    "resume": "build/main.js"
  },
  "dependencies": {
    "async": "^3.2.0",
    "browser-sync": "^2.26.7",
    "btoa": "^1.2.1",
    "chalk": "^4.1.0",
    "commander": "^6.0.0",
    "dotenv": "^8.2.0",
    "jsonlint": "^1.6.3",
    "jsonresume-theme-even": "^0.5.2",
    "puppeteer": "^5.2.1",
    "read": "^1.0.7",
    "resume-schema": "^1.0.0",
    "superagent": "^6.0.0",
    "util.promisify": "^1.0.1",
    "z-schema": "^4.2.3",
    "z-schema-errors": "^0.2.1"
  },
  "devDependencies": {
    "@babel/cli": "7.12.8",
    "@babel/core": "7.12.9",
    "@babel/eslint-parser": "7.12.1",
    "@babel/node": "7.12.6",
    "@babel/plugin-proposal-optional-chaining": "7.12.7",
    "@babel/preset-env": "7.12.7",
    "babel-eslint": "10.1.0",
    "eslint": "7.14.0",
    "husky": "4.3.5",
    "lint-staged": "10.5.3",
    "mocha": "8.1.1",
    "should": "^13.2.3",
    "prettier": "2.2.0"
  }
}
